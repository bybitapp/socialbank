  "engines": {
    "node": ">=8.1.2"
  },
  "scripts": {
    "dev": "concurrently 'PORT=8088 NODE_PATH=. nodemon --watch lib server.js' 'node scripts/start.js'",
    "start": "NODE_PATH=. node server.js",
    "build": "node scripts/build.js",
    "test": "NODE_PATH=. node scripts/test.js --env=jsdom",
    "postinstall": "yarn run build"
  },
  "proxy": "http://localhost:8088",
  "dependencies": {
    "@google/maps": "^0.4.1",
    "autoprefixer": "7.1.1",
    "axios": "^0.16.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^3.5.0",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
