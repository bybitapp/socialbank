    "jest": "20.0.4",
    "joi": "^10.6.0",
    "lodash": "^4.17.4",
    "lusca": "^1.4.1",
    "mailchimp-client": "https://github.com/pearlshare/mailchimp-client",
    "material-design-lite": "^1.3.0",
    "moment": "^2.18.1",
    "mongoose": "^4.11.0",
    "nodemailer": "^4.0.1",
    "object-assign": "4.1.1",
