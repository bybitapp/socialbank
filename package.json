    "react-modal": "^2.2.0",
    "react-redux": "^5.0.5",
    "react-redux-toastr": "7.0.0",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "recompose": "^0.23.5",
    "redux": "^3.7.1",
    "redux-form": "^6.8.0",
